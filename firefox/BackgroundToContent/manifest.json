{

  "manifest_version": 2,
  "name": "BackgroundToContent",
  "version": "1.0",

  "description": "Background to Content.",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
      "default_icon": "action_button.svg"
  }

}

